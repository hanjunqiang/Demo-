#import "XWSwitchItem.h"

@implementation XWSwitchItem

+(instancetype)itemWithIcon:(NSString *)icon title:(NSString *)title
{
    XWSwitchItem *item = [[XWSwitchItem alloc]init];
    item.icon = icon;
    item.title = title;
    
    return item;
}


@end
